datasource db {
    provider             = "mysql"
    referentialIntegrity = "prisma"
    url                  = env("DATABASE_URL")
    shadowDatabaseUrl    = env("SHADOW_DATABASE_URL")
}

generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["referentialIntegrity"]
    output          = "../src/database/client/generated"
}
